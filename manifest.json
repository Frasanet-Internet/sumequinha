{
  "name": "Sumequinha",
  "description": "Um timer simples que muda as cores dos itens na lista de atendimentos da plataforma Opa! chat.",
  "version": "0.4",
  "manifest_version": 3,
  "action": {
    "default_icon": {
      "32": "img/sumequinha_32.png",
      "64": "img/sumequinha_64.png",
      "128": "img/sumequinha_128.png",
      "1024": "img/sumequinha_1024.png"
    }
  },
  "background": {
    "service_worker": "public/js/rinnegan.js"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ]
}
